"~/.tools/volume +5%"
   XF86AudioRaiseVolume
"~/.tools/volume -5%"
   XF86AudioLowerVolume
"~/.tools/volume-mute"
   XF86AudioMute
"pactl set-source-mute @DEFAULT_SOURCE@ toggle"
   XF86AudioMicMute
"~/.tools/brightness 10%+"
   XF86MonBrightnessUp
"~/.tools/brightness 10%-"
   XF86MonBrightnessDown
